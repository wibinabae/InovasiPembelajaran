<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nabaetechnology.inovasipembelajaran">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InovasiPembelajaran"
        tools:targetApi="31">
        <activity
            android:name=".TentangActivity"
            android:exported="false" />
        <activity
            android:name=".MateriActivity"
            android:exported="false" />
        <activity
            android:name=".KonsepActivity"
            android:exported="false" />
        <activity
            android:name=".PemantikActivity"
            android:exported="false"
            android:label="@string/title_activity_pertanyaan_pemantik" />
        <activity
            android:name=".PembelajaranActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>